{
	"name": "Unpivot",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "TestDataset",
						"type": "DatasetReference"
					},
					"name": "TestData"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "SinkDataSet",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "unpivot1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          PO as string,",
				"          Vendor as string,",
				"          Fruits as string,",
				"          Amount as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> TestData",
				"TestData unpivot(output(",
				"          Fruits as string,",
				"          Amount as string",
				"     ),",
				"     ungroupBy(PO,",
				"          Vendor),",
				"     lateral: true,",
				"     ignoreNullPivots: false) ~> unpivot1",
				"unpivot1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          depid as string,",
				"          depname as string",
				"     ),",
				"     partitionFileNames:['UnpivotData.csv'],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> sink1"
			]
		}
	}
}